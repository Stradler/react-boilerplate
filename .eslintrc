{
    "parser": "babel-eslint",
    "extends": "react",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "space-before-function-paren": "off",
        "comma-dangle": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
